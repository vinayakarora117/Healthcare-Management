        <name>Fetch Available Slots</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || !newValue) return;

    var testType = g_form.getValue('u_test_type');
    if (!testType) return g_form.addErrorMessage("Select test type first.");

    var ga = new GlideAjax('LabUtilsPractice');
    ga.addParam('sysparm_name', 'getAvailableSlots');
    ga.addParam('sysparm_test_type', testType);
    ga.addParam('sysparm_scheduled_date', newValue);

    ga.getXMLAnswer(function(response) {
        var slots = JSON.parse(response || '[]');
        if (!slots.length) {
            g_form.addErrorMessage("No available slots found.");
        } else {
            g_form.addInfoMessage("Available slot " );
        }
    });
}
]]></script>
