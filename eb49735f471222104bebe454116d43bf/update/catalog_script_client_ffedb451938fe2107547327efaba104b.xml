        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }
   var ga=new GlideAjax('InsuranceUtilsPractice');
   ga.addParam('sysparam_name','detailByID');
   ga.addParam('sys_patient_id',newValue);
   ga.getXMLAnswer(function(response){
	var data=JSON.parse(response || '[]');
	if(!data.length){
		g_form.addInfoMessage('No insurance records found for this patient.');
		return;
	}
	var msg='Insurance details:';
	for (var i = 0; i < data.length; i++) {
            msg += "- Policy: " + data[i].policy + ", Status: " + data[i].status + ", Coverage: " + data[i].details ;}
			g_form.addInfoMessage(msg);
   });
   //Type appropriate comment here, and begin script below
   
}]]></script>
